import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class ClaimRequestDTO {
	
	long id;
	String synopsis;
	double price;
	long patientId;
	long hospitalId;
	long insuranceCompId;
	String status;
	String statusMessage;
	String lastUpdatedBy;
}
